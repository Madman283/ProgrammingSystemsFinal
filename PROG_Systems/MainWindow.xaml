<Window x:Class="PROG_Systems.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PROG_Systems"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <TextBlock x:Name="TextNotes" HorizontalAlignment="Left" Margin="65,29,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="163" Width="335"/>
        <Button x:Name="ChangeTime" Content="Button" HorizontalAlignment="Left" Margin="679,29,0,0" VerticalAlignment="Top" Height="41" Width="77" Click="ChangeTime_Click"/>
        <TextBlock  x:Name="EnvironmentName" HorizontalAlignment="Left" Margin="30,289,0,0" TextWrapping="Wrap" Text="{Binding Path=Name}" VerticalAlignment="Top" Height="68" Width="404" TextAlignment="Center" FontFamily="Lucida Bright" FontSize="24"/>
    </Grid>
</Window>
